<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>theOasis</display-name>
  <listener>
    <listener-class>ServerListener</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>forceEncoding</param-name>
      <param-value>true</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>theOasis</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description>
		</description>
    <display-name>CheckAgree</display-name>
    <servlet-name>CheckAgree</servlet-name>
    <servlet-class>com.theOasis.server.controller.CheckAgreeController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>theOasis</servlet-name>
    <url-pattern>*.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>JoinServlet</display-name>
    <servlet-name>JoinServlet</servlet-name>
    <servlet-class>com.theOasis.server.controller.JoinServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JoinServlet</servlet-name>
    <url-pattern>/join.oasis</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckAgree</servlet-name>
    <url-pattern>/CheckAgree.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>file</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>file</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ias</display-name>
    <servlet-name>ias</servlet-name>
    <servlet-class>com.theOasis.servlet.InitAjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ias</servlet-name>
    <url-pattern>/ajax.init</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>cas</display-name>
    <servlet-name>cas</servlet-name>
    <servlet-class>com.theOasis.servlet.ClickAjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cas</servlet-name>
    <url-pattern>/ajax.neo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>das</display-name>
    <servlet-name>das</servlet-name>
    <servlet-class>com.theOasis.servlet.DeleteAjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>das</servlet-name>
    <url-pattern>/ajax.del</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>mas</display-name>
    <servlet-name>mas</servlet-name>
    <servlet-class>com.theOasis.servlet.MovedAjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mas</servlet-name>
    <url-pattern>/ajax.move</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>cav</display-name>
    <servlet-name>cav</servlet-name>
    <servlet-class>com.theOasis.servlet.CreateAjaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cav</servlet-name>
    <url-pattern>/ajax.create</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GroupNameServlet</display-name>
    <servlet-name>GroupNameServlet</servlet-name>
    <servlet-class>com.theOasis.server.controller.GroupNameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GroupNameServlet</servlet-name>
    <url-pattern>/groupName.oasis</url-pattern>
  </servlet-mapping>
</web-app>